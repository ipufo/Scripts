function objectinsert(path, permaornot, cframe)
    if permaornot then
        game:GetService("ReplicatedStorage"):WaitForChild("StarPlatinumStuff"):WaitForChild("SP3"):FireServer("Hit1", {Player.Character.Humanoid, Vector3.new(), cframe, Vector3.new(), 0, 0, 0, path, nil, math.huge, 9e99}, {workspace.MIHBass:Play()}, {9e99})
    else
        game:GetService("ReplicatedStorage"):WaitForChild("StarPlatinumStuff"):WaitForChild("SP3"):FireServer("Hit1", {Player.Character.Humanoid, Vector3.new(), cframe, Vector3.new(), 0, 0, 0, path, "Effect1", 2, 1}, {workspace.MIHBass:Play()}, {9e99})
    end
end

function Lerp(a, b, t)
	return a + (b - a) * t
end
function Quad(Time, A, B, C)
    local Lerp1 = A:Lerp(B, Time)
    local Lerp2 = B:Lerp(C, Time)
    local Result = Lerp1:Lerp(Lerp2, Time)
	return Result
end
function AnimationPlay(id)
    local Animation = Instance.new("Animation")
    Animation.AnimationId = "rbxassetid://"..id
    local AnimPlayer = Player.Character.Humanoid:LoadAnimation(Animation)
    AnimPlayer:Play()
end
