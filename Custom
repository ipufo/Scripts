                    ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -25), 25, .1, Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -1).Position, "rbxassetid://3370684035", 0.075, Color3.fromHSV(tick()%2/2,1,1), "rbxassetid://", 0, 100)
                    brexittacklevelocity.Velocity = Player.Character.HumanoidRootPart.CFrame.lookVector * 130 + Vector3.new(0, 1, 0)
                    for Index, Entity in pairs(workspace.Entities:GetChildren()) do
                        if Entity:FindFirstChild("HumanoidRootPart") and Entity.Name ~= Player.Name then
                            if (Player.Character.HumanoidRootPart.Position - Entity.HumanoidRootPart.Position).Magnitude < 7.5 then
                                ReplicatedStorage.Knock:FireServer(Entity.Humanoid, true)
                                ReplicatedStorage.Damage11Sans:FireServer(Entity.Humanoid, Entity.HumanoidRootPart.CFrame, 0, .1, Player.Character.HumanoidRootPart.CFrame.LookVector * 150 +Vector3.new(0, 100, 0), 9e99, "rbxassetid://7779900880", 1, .5)
                            end
                        end
                    end
                    task.wait()
                until not brexittackle
            end
        elseif Input == "T" then
            if not Posing then
                local animation3 = Instance.new("Animation")
                animation3.AnimationId = "rbxassetid://3674055951"
                local animationplayer3 = Player.Character.Humanoid:LoadAnimation(animation3)
                animationplayer3:Play()
                freezebreath = true
                task.wait(.65)
                animationplayer3:AdjustSpeed(0)
                task.spawn(function()
                    repeat
                        for index = 1, 11 do
                            ReplicatedStorage.Damage12Sans:FireServer(Player.Character.Humanoid, Player.Character.Head.CFrame*CFrame.new(0, 0, -index * 2), 0.000001, 0, Vector3.new(), index / 100, "", 0, 0)
                        end
                        for Index, Entity in pairs(workspace.Entities:GetChildren()) do
                            if Entity:FindFirstChild("HumanoidRootPart") and Entity.Name ~= Player.Name and not Entity.Name:match("ipufo") then
                                if (Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -20).Position - Entity.HumanoidRootPart.Position).Magnitude < 20 then
                                    for Index, Part in pairs(Entity:GetDescendants()) do
                                        if Part.ClassName:match("Part") and Part.ClassName ~= "ParticleEmitter" and Part.Anchored == false then
                                            ReplicatedStorage.Anchor:FireServer(Part, true)
                                        end
                                    end
                                    ReplicatedStorage.Damage11Sans:FireServer(Entity.Humanoid, Entity.HumanoidRootPart.CFrame, 5, 0, Vector3.new(), 9e99, "rbxassetid://268249319", 1, 100)
                                end
                            end
                        end
                        task.wait(.1)
                    until not freezebreath
                end)
            end
        elseif Input == "Y" then
            if not Posing then
                local animation3 = Instance.new("Animation")
                animation3.AnimationId = "rbxassetid://3674055951"
                local animationplayer3 = Player.Character.Humanoid:LoadAnimation(animation3)
                animationplayer3:Play()
                meltbreath = true
                task.wait(.65)
                animationplayer3:AdjustSpeed(0)
                task.spawn(function()
                    repeat
                        for index = 1, 11 do
                            ReplicatedStorage.Damage12:FireServer(Player.Character.Humanoid, Player.Character.Head.CFrame*CFrame.new(0, 0, -index * 2), 0.000001, 0, Vector3.new(), index / 100, "", 0, 0)
                        end
                        for Index, Entity in pairs(workspace.Entities:GetChildren()) do
                            if Entity:FindFirstChild("HumanoidRootPart") and Entity.Name ~= Player.Name and not Entity.Name:match("ipufo") then
                                if (Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -20).Position - Entity.HumanoidRootPart.Position).Magnitude < 20 then
                                    for Index, Part in pairs(Entity:GetDescendants()) do
                                        if Part.ClassName:match("Part") and Part.ClassName ~= "ParticleEmitter" and Part.Anchored == true then
                                            ReplicatedStorage.Anchor:FireServer(Part, false)
                                        end
                                    end
                                    ReplicatedStorage.Damage11Sans:FireServer(Entity.Humanoid, Entity.HumanoidRootPart.CFrame, 25, 0, Vector3.new(), 9e99, "rbxassetid://9068935533", 1, 100)
                                end
                            end
                        end
                        task.wait(.1)
                    until not meltbreath
                end)
            end
        elseif Input == "G" then
            if not Posing then
                local animation4 = Instance.new("Animation")
                animation4.AnimationId = "rbxassetid://" .. pose["stwsit"]
                local animationplayer4 = Player.Character.Humanoid:LoadAnimation(animation4)
                animationplayer4:Play()
                task.wait(.5)
                for Index, Entity in pairs(workspace.Entities:GetChildren()) do
                    if Entity:FindFirstChild("HumanoidRootPart") and Entity.Name ~= Player.Name and not Entity.Name:match("ipufo") then
                        if (Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -20).Position - Entity.HumanoidRootPart.Position).Magnitude < 40 then
                            task.spawn(function()
                                local Middle = Player.Character.HumanoidRootPart.Position:Lerp(Entity.HumanoidRootPart.Position, 0.5) + Vector3.new(0, (Player.Character.HumanoidRootPart.Position - Entity.HumanoidRootPart.Position).Magnitude / 2, 0)
                                for Time = 0, 1, 1/50 do
                                    game.ReplicatedStorage.Damage12Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid, CFrame.new(Quad(Time, Player.Character.HumanoidRootPart.Position, Middle, Entity.HumanoidRootPart.Position)), 0.01, 0, Vector3.zero, 0.0075, "", 1, 1)
                                    task.wait()
                                end
                                for Index = 1, 10 do
                                    for Index2 = 1, 3 do
                                        ReplicatedStorage.Damage11Sans:FireServer(Entity.Humanoid, Entity.HumanoidRootPart.CFrame, 99, 0, Vector3.zero, 9e99, "rbxassetid://5363075887", 1, 100)
                                    end
                                    task.wait(.1)
                                end
                                task.wait(0.5)
                                for Time = 0, 1, 1/50 do
                                    --ReplicatedStorage.Damage11Sans:FireServer(Entity.Humanoid, Entity.HumanoidRootPart.CFrame, 1, .1, Quad(Time, EndPosition, Middle, StartPosition), 9e99, "rbxassetid://5363075887", 0, 100)
                                    game.ReplicatedStorage.Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, CFrame.new(Quad(Time, Entity.HumanoidRootPart.Position, Middle, Player.Character.HumanoidRootPart.Position)), 0.01, 0, Vector3.zero, 0.0075, "", 1, 1)
                                    task.wait()
                                end
                            end)
                        end
                    end
                end
            end
        elseif Input == "Z" then -- mouse tp
            if (Player.Character.HumanoidRootPart.Position - Mouse.Hit.Position).Magnitude < 1000 then
                ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame, 25, 0, Vector3.zero, "", 0.095, Color3.new(255, 255, 0), "rbxassetid://13775494", 1, 5)
                for index = 0, 1, 1 / (Player.Character.HumanoidRootPart.Position - Mouse.Hit.Position).Magnitude * 1.5 do
                    ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, CFrame.new(Lerp(Player.Character.HumanoidRootPart.Position, Mouse.Hit.Position + Vector3.new(0, 3, 0), index)), 0, 0, Vector3.zero, "", 0, Color3.new(255, 255, 0), "rbxassetid://13775494", 0, 5)
                end
                Player.Character.HumanoidRootPart.CFrame = Mouse.Hit * CFrame.new(0, 3, 0)
                ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, Mouse.Hit * CFrame.new(0, 3, 0), 25, 0, Vector3.zero, "", 0.075, Color3.new(255, 255, 0), "rbxassetid://13775494", 0, 5)    
            end
        elseif Input == "V" then
            if (Player.Character.HumanoidRootPart.Position - Mouse.Hit.Position).Magnitude < 1000 then
                ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame, 25, 0, Vector3.zero, "", 0.055, Color3.new(255, 255, 0), "rbxassetid://13775494", 1, 5)
                for index = 0, 1, 1 / (Player.Character.HumanoidRootPart.Position - Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -40).Position).Magnitude * 1.5  do
                    ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, CFrame.new(Lerp(Player.Character.HumanoidRootPart.Position, Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -40).Position, index)), 0, 0, Vector3.zero, "", 0, Color3.new(255, 255, 0), "rbxassetid://13775494", 0, 5)
                end
                Player.Character.HumanoidRootPart.CFrame *= CFrame.new(0, 0, -40) 
                ReplicatedStorage.Damage:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame, 25, 0, Vector3.zero, "", 0.025, Color3.new(255, 255, 0), "rbxassetid://13775494", 0, 5)    
            end
        elseif Input == "P" then
            Posing = not Posing
            local RandomSong = {
                ["crabwave"] = "5410086218",
                ["chicken nugget dreamland"] = "9245561450",
                ["crash bandicoot"] = "11808876109",
                ["freak"] = "6703926669",
                ["internet"] = "6708444383",
                ["freak"] = "6703926669",
                ["smug"] = "6753817247"
            }
            local played = RandomSong["internet"]
            if Posing then
                local animation = Instance.new("Animation")
                animation.AnimationId = "rbxassetid://5447727344"
                local animplay = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(animation)
                local AuraValue = 0
                local AuraValue2 = 0
                local UpDownValue = 0
                runnin = false
                Player.Character.Humanoid.WalkSpeed = 0
                StandTransparency(0)
                animplay:Play()
                task.wait(2)
                --ReplicatedStorage.Menacing:FireServer(Posing)
                AudioValues:PlayAudio(played, 16, true, 1)
                animplay:AdjustSpeed(0)
                task.spawn(function()
                    repeat
                        AuraValue += 10
                        AuraValue2 -= 10
                       ReplicatedStorage.Damage12:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame * CFrame.new(8 * -math.sin(AuraValue), -1.5, 8 * -math.cos(AuraValue)), 0.0000001, 0, Vector3.zero, 0.0075, "", 0, 0)
                       ReplicatedStorage.Damage12:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame * CFrame.new(8 * math.sin(AuraValue), -1.5, 8 * math.cos(AuraValue)), 0.0000001, 0, Vector3.zero, 0.0075, "", 0, 0)
                       ReplicatedStorage.Damage12Sans:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame * CFrame.new(16 * -math.cos(AuraValue), -1.5, 16 * -math.sin(AuraValue)), 0.0000001, 0, Vector3.zero, 0.0075, "", 0, 0)
                       ReplicatedStorage.Damage12Sans:FireServer(Player.Character.Humanoid, Player.Character.HumanoidRootPart.CFrame * CFrame.new(16 * math.cos(AuraValue), -1.5, 16 * math.sin(AuraValue)), 0.0000001, 0, Vector3.zero, 0.0075, "", 0, 0)
                        task.wait()
                    until not Posing
                end)
            elseif not Posing then
                runnin = true
                AudioValues:StopAudio(played)
                StandTransparency(1)
                ReplicatedStorage.Menacing:FireServer(Posing)
                StopAnimations()
            end
        end
    end
end)

UserInputService.InputEnded:Connect(function(Key, IsTyping)
    if not IsTyping then
        local Input = Key.KeyCode.Name
        if not Posing then
            if Input == "Q" then
                barrage = false
                StopAnimations()
            elseif Input == "C" then
                brexittackle = false
                for index, velocity in pairs(Player.Character.Torso:GetChildren()) do
                    if velocity.Name:match("BREXIT") then
                        velocity:Destroy()
                    end
                end
                runnin = true
                StopAnimations()
            elseif Input == "T" then
                StopAnimations()
                freezebreath = false
           --[[ elseif Input == "N" then
                StopAnimations()
                Dodging = false ]]--
            elseif Input == "Y" then
                StopAnimations()
                meltbreath = false
            elseif Input == "B" then
                StopAnimations()
                for Index, HitboxPart in pairs(Player.Character:GetChildren()) do
                    if HitboxPart.Name:match("PartsInPartHitbox") then
                        HitboxPart:Destroy()
                    end
                end
                Cumming = false
            end
        end
    end
end)
